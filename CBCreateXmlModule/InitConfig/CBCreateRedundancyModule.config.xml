<?xml version='1.0' encoding='ISO-8859-1'?>
<config version='0.1'>
	<components>
		<component>
			<name>palpage_module</name><!-- Nom du module. Attention aux caractères interdits par ControlBuild -->
			<refFile></refFile><!-- Nom du fichier a traiter en entrée. Ceci doit être un fichier de variables ControlBuild au format XML -->
			<outDir>**NAME**</outDir><!-- Répertoire de sortie. **NAME** prends le nom du module comme sortie -->
			<template>Default</template><!-- Name of template to use -->
			<rules>
				<rule> <!-- Règle de réecriture -->
					<rewritingRule>
						<matchExpr><![CDATA[]]></matchExpr><!-- Expression régulière a détecter -->
						<replaceExpr><![CDATA[]]></replaceExpr><!-- Motif de remplacement de l'expression régulière -->
					</rewritingRule>
					<commentRule>
						<matchExpr><![CDATA[]]></matchExpr><!-- Expression régulière a détecter -->
						<replaceExpr><![CDATA[]]></replaceExpr><!-- Motif de remplacement de l'expression régulière -->
					</commentRule>
				</rule>
			</rules>
		</component>
	</components>
</config>